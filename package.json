    "eslint-config-next": "14.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-storybook": "^10.0.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8.4.33",
    "prettier": "^3.7.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass": "^1.70.0",
    "storybook": "^10.0.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
